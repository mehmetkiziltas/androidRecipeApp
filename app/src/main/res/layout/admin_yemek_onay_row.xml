<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textAdminYemekOnayYemekAdi"
        android:layout_width="154dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="131dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="126dp"
        android:gravity="center"
        android:text="TextView"
        android:textColor="@color/new_primarycolor"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageViewAdminYemekOnayYemekResmi"
        android:layout_width="164dp"
        android:layout_height="107dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="124dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textAdminYemekOnayYemekAdi"
        app:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/textViewAdminYmekOnayYemekOzelligi"
        android:layout_width="158dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="123dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="130dp"
        android:gravity="center"
        android:text="TextView"
        android:textColor="@color/new_primarycolor"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewAdminYemekOnayYemekResmi" />

    <TextView
        android:id="@+id/textView3AdminYemekOnayYemekAdi"
        android:layout_width="132dp"
        android:layout_height="29dp"
        android:layout_marginStart="139dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="140dp"
        android:gravity="center"
        android:text="MALZEMELER"
        android:textColor="@color/new_primarycolor"
        android:textColorHint="@color/new_primarycolor"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAdminYmekOnayYemekOzelligi" />

    <EditText
        android:id="@+id/textViewAdminYemekOnayYemekMalzemeler"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="15dp"
        android:inputType="textMultiLine"
        android:text="TextView"
        android:textColor="@color/new_primarycolor"
        android:textColorHint="@color/new_primarycolor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3AdminYemekOnayYemekAdi" />

    <EditText
        android:id="@+id/textViewAdminYemekOnayYapilisDetay"
        android:layout_width="383dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="14dp"
        android:inputType="textMultiLine"
        android:text="TextView"
        android:textColor="@color/new_primarycolor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4AdminYemekOnayYemekAdi" />

    <EditText
        android:id="@+id/textView4AdminYemekOnayYemekAdi"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="131dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="140dp"
        android:gravity="center"
        android:text="YAPILIÅž"
        android:textColor="@color/new_primarycolor"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAdminYemekOnayYemekMalzemeler" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_marginBottom="147dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAdminYemekOnayYapilisDetay"
        app:layout_constraintVertical_bias="0.0">

        <Button
            android:id="@+id/AdminOnaylaButton"
            android:layout_width="150dp"
            android:layout_height="70dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:text="Onayla"
            android:textColor="@color/new_primarycolor"
            android:textSize="25dp">

        </Button>

        <Button
            android:id="@+id/AdminSilButton"
            android:layout_width="150dp"
            android:layout_height="70dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:text="Sil"
            android:textColor="@color/new_primarycolor"
            android:textSize="25dp">

        </Button>
    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>
